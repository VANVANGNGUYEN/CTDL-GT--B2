void stack_data::push (int a)  // them 1 phan tu
{
    if(top >= 5)
    {
        cout << "Overflow\n";
    }
    else
    {
        data[++top] = a;   
    }
}
int stack_data::pop ()  //xoa 1 phan tu
{
    if(top < 0)
    {
        cout << "Underflow\n";
        return 0;
    }
    else
    {
        int pop = data[top--];
        return pop;
    }
}
